{
  "expo": {
    "name": "HabiTora",
    "slug": "habi-tora",
    "description": "自動チェックで楽々習慣化！シンプルな習慣トラッカー",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.junyamaguchi.habitora"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.junyamaguchi.habitora",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "organization": "jy-team",
          "project": "habitora",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/app-icon.png",
          "dark": {
            "image": "./assets/images/app-icon.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ecd5fd4a-fb90-41bc-882f-dd6887dc6d39"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/ecd5fd4a-fb90-41bc-882f-dd6887dc6d39"
    },
    "runtimeVersion": "1.0.0"
  }
}
